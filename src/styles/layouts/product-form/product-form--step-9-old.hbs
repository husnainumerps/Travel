                            {{> '@form-label' modifier="rlr-form-label" type="radio" formLabel="Individual Seats" forLabel="individual_seats"}}
                        </li>
                        <li class="form-check">
                            {{> '@radio' inputType="radio" isRequired="required" radioName="price_activity" value="Private Group" radioId="private_group"}}
                            {{> '@form-label' modifier="rlr-form-label" type="radio" formLabel="Private Group" forLabel="private_group"}}
                        </li>
                    </ul>
            </div>
            <div class="rlr-fieldrow__item rlr-fieldrow__item--multiple">
                <div
                    class="rlr-fieldrow__options rlr-fieldrow__options--lg-spacer js-fieldrow__item">
                    {{> '@form-label' formLabel="Are the prices based on date range?" type="dark"}}
                    <ul class="rlr-radios">
                        <li class="form-check">
                            {{> '@radio' inputType="radio" isRequired="required" radioName="date_range" value="No" radioId="rlr-date-range-no"}}
                            {{> '@form-label' modifier="rlr-form-label" type="radio" formLabel="No" forLabel="rlr-date-range-no"}}
                        </li>
                        <li class="form-check">
                            {{> '@radio' inputType="radio" isRequired="required" radioName="date_range" value="Yes" radioId="rlr-date-range-yes"}}
                            {{> '@form-label' modifier="rlr-form-label" type="radio" formLabel="Yes" forLabel="rlr-date-range-yes"}}
                        </li>
                    </ul>
                    <div class="rlr-input-groups js-date-ranges">
                        {{> '@input-group' modifier="rlr-input-group__datefield rlr-js-linked" range="range-start" inputType="date" }}
                        {{> '@input-group' modifier="rlr-input-group__datefield rlr-js-linked" range="range-end" inputType="date" }}
                    </div>
                </div>
                <div
                    class="rlr-fieldrow__options rlr-fieldrow__options--lg-spacer js-fieldrow__item">
                    {{> '@form-label' formLabel="Are the prices based on age groups?" type="dark"}}
                    <ul class="rlr-radios">
                        <li class="form-check">
                            {{> '@radio' inputType="radio" isRequired="required" radioName="age_groups" value="No" radioId="rlr-age-groups-no"}}
                            {{> '@form-label' modifier="rlr-form-label" type="radio" formLabel="No" forLabel="rlr-age-groups-no"}}
                        </li>
                        <li class="form-check">
                            {{> '@radio' inputType="radio" isRequired="required" radioName="age_groups" value="Yes" radioId="rlr-age-groups-yes"}}
                            {{> '@form-label' modifier="rlr-form-label" type="radio" formLabel="Yes" forLabel="rlr-age-groups-yes"}}
                        </li>
                    </ul>
                    <div class="rlr-input-groups">
                        {{> '@input-group' inputType="number" modifier="rlr-input-group__numberfield rlr-js-linked" icon='<svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M12 20.572a8.572 8.572 0 1 0 0-17.143 8.572 8.572 0 0 0 0 17.143z" stroke="#546179" stroke-width="2" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round"/><path d="M9.809 9.342a2.235 2.235 0 0 1 4.343.745c0 1.49-2.235 2.235-2.235 2.235M11.979 16.343h.007" stroke="#546179" stroke-width="2" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round"/></svg>' inputType="number" placeholderText="Price per Adult 18+"}}
                        {{> '@input-group' inputType="number" modifier="rlr-input-group__numberfield rlr-js-linked" icon='<svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M12 20.572a8.572 8.572 0 1 0 0-17.143 8.572 8.572 0 0 0 0 17.143z" stroke="#546179" stroke-width="2" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round"/><path d="M9.809 9.342a2.235 2.235 0 0 1 4.343.745c0 1.49-2.235 2.235-2.235 2.235M11.979 16.343h.007" stroke="#546179" stroke-width="2" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round"/></svg>' inputType="number" placeholderText="Price per Child 7-17"}}
                        {{> '@input-group' inputType="number" modifier="rlr-input-group__numberfield rlr-js-linked" icon='<svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M12 20.572a8.572 8.572 0 1 0 0-17.143 8.572 8.572 0 0 0 0 17.143z" stroke="#546179" stroke-width="2" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round"/><path d="M9.809 9.342a2.235 2.235 0 0 1 4.343.745c0 1.49-2.235 2.235-2.235 2.235M11.979 16.343h.007" stroke="#546179" stroke-width="2" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round"/></svg>' inputType="number" placeholderText="Price per Infant 0-3"}}
                        {{> '@input-group' inputType="number" modifier="rlr-input-group__numberfield rlr-js-linked" icon='<svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M12 20.572a8.572 8.572 0 1 0 0-17.143 8.572 8.572 0 0 0 0 17.143z" stroke="#546179" stroke-width="2" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round"/><path d="M9.809 9.342a2.235 2.235 0 0 1 4.343.745c0 1.49-2.235 2.235-2.235 2.235M11.979 16.343h.007" stroke="#546179" stroke-width="2" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round"/></svg>' inputType="number" placeholderText="Price per Senior 65+"}}
