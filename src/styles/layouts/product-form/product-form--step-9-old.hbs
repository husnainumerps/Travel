                </div>
            </div>
            {{> '@button' modifier="text-button rlr-button--product-form-repeater js-repeater" shape="rounded" size="small" btnBgColor="transparent" btnTextColor="gray" btnText="+ Add another theme"}}
            <div class="rlr-fieldrow__item">
                <div class="rlr-fieldrow__item rlr-fieldrow__item--multiple">
                    {{> '@form-label' formLabel="Product Addons" type="dark"}}
                    <div class="rlr-input-groups rlr-custom-inputs">
                        <div class="rlr-custom-input rlr-custom-input--option">
                            {{> '@form-select' optionSelected="Select" option1="Health and Fitness" option2="Outdoor and Adventure" option3="Religion" option4="Food and Drink" }}
                        </div>
                        {{> '@input-group' inputType="number" modifier="rlr-input-group__numberfield rlr-custom-input-option" icon='<svg width="14" height="12" viewBox="0 0 14 12" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path d="M5.394 11.123a.698.698 0 0 1-.99 0L.45 7.17a1.05 1.05 0 0 1 0-1.485l.495-.495a1.05 1.05 0 0 1 1.486 0l2.468 2.468 6.67-6.67a1.05 1.05 0 0 1 1.485 0l.495.496c.41.41.41 1.075 0 1.485l-8.155 8.155z" fill="#fff"/>
                        </svg>
                        ' placeholderText="Price"}}
                    </div>
                    {{> '@button' modifier="text-button rlr-button--product-form-repeater js-repeater" shape="rounded" size="small" btnBgColor="transparent" btnTextColor="gray" btnText="+ Add another theme"}}
                </div>
            </div>
        </div>
    </div>
</fieldset>